{
    "tailwindCSS.experimental.configFile": "src/app/tailwind.config.js",
    "FSharp.inlayHints.disableLongTooltip": true,
    "editor.inlayHints.enabled": "offUnlessPressed",
    "FSharp.inlayHints.enabled": false,
    "FSharp.inlayHints.parameterNames": false,
    "FSharp.inlayHints.typeAnnotations": false,
    "FSharp.lineLens.enabled": "never",
    "FSharp.pipelineHints.enabled": false,
    "files.exclude": {
        "**/bin": true,
        "**/node_modules": true,
        "**/obj": true
    }
}
